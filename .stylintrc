{
	"blocks": false,
	"brackets": "always",
	"colons": "always",
	"colors": "always",
	"commaSpace": "always",
	"commentSpace": "always",
	"cssLiteral": "never",
	"customProperties": [],
	"depthLimit": false,
	"duplicates": true,
	"efficient": "always",
	"exclude": [],
	"extendPref": "@extends",
	"globalDupe": true,
	"groupOutputByFile": true,
	"indentPref": false,
	"leadingZero": "always",
	"maxErrors": false,
	"maxWarnings": false,
	"mixed": true,
	"mixins": [],
	"namingConvention": false,
	"namingConventionStrict": false,
	"none": "always",
	"noImportant": false,
	"parenSpace": "never",
	"placeholders": "always",
	"prefixVarsWithDollar": "always",
	"quotePref": "double",
	"reporterOptions": {
		"columns": ["lineData", "severity", "description", "rule"],
		"columnSplitter": "	",
		"showHeaders": false,
		"truncate": true
	},
	"semicolons": "never",
	"sortOrder": ["grouped", "alphabetical"],
	"stackedProperties": "never",
	"trailingWhitespace": "never",
	"universal": false,
	"valid": true,
	"zeroUnits": "never",
	"zIndexNormalize": false,
	"stylusSupremacy.selectorSeparator": ",\n",
	"stylusSupremacy.insertNewLineAroundBlocks": true
}
